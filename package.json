{
  "name": "@creativetimofficial/react-tagsinput",
  "version": "3.19.0",
  "description": "Highly customizable React component for inputing tags",
  "main": "react-tagsinput.js",
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "16.4.0",
    "mocha": "8.2.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-autosuggest": "10.0.3",
    "react-dom": "17.0.1",
    "react-input-autosize": "2.2.2",
    "reactpack": "0.9.0",
    "sinon": "9.2.1",
    "standard": "16.0.3"
  },
  "scripts": {
    "test": "standard src/index.js && mocha --compilers js:babel-register",
    "build": "babel --module-id ReactTagsInput -m umd src/index.js > react-tagsinput.js",
    "build-example": "reactpack --no-extract --no-source-map --no-html example/index.js example/",
    "build-example-watch": "reactpack -w --no-extract --no-source-map --no-html example/index.js example/",
    "coverage": "istanbul cover --report text --report html _mocha -- --compilers js:babel-register",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/creativetimofficial/react-tagsinput"
  },
  "keywords": [
    "react",
    "tags",
    "input",
    "component",
    "javascript",
    "react-component"
  ],
  "author": "Ola Holmstr√∂m",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/react-tagsinput/issues"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "homepage": "https://github.com/creativetimofficial/react-tagsinput"
}
